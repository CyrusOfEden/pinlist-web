  "engines": {
    "node": ">= 10.0.0",
    "yarn": ">= 1.0.0"
  },
  "web": "dist/web/index.html",
  "targets": {
    "web": {
      "context": "browser",
      "isLibrary": false,
      "engines": {
        "browsers": [
          "last 2 versions"
        ]
      }
    }
  },
  "scripts": {
    "dev:web": "yarn webpack-dev-server --config webpack.web.js --watch",
    "build:web": "NODE_ENV=production yarn webpack --config webpack.web.js",
    "deploy:web": "yarn build:web && firebase deploy --only hosting",
    "dev:chrome": "NODE_ENV=development TARGET_BROWSER=chrome yarn webpack --config webpack.extension.js --watch",
    "build:chrome": "NODE_ENV=production TARGET_BROWSER=chrome yarn webpack --config webpack.extension.js",
    "dev:firefox": "NODE_ENV=development TARGET_BROWSER=firefox yarn webpack --config webpack.extension.js --watch",
    "build:firefox": "NODE_ENV=production TARGET_BROWSER=firefox yarn webpack --config webpack.extension.js",
    "dev:opera": "NODE_ENV=development TARGET_BROWSER=opera yarn webpack --config webpack.extension.js --watch",
    "build:opera": "NODE_ENV=production TARGET_BROWSER=opera yarn webpack --config webpack.extension.js",
    "build:extension": "yarn run build:chrome && yarn run build:firefox && yarn run build:opera",
    "types:check": "yarn tsc -w",
    "lint": "yarn eslint . --ext .ts,.tsx",
    "lint:fix": "yarn eslint . --ext .ts,.tsx --fix"
  },
