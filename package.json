{
  "name": "pinlist",
  "version": "0.0.1",
  "description": "Web extension starter using react and typescript",
  "private": true,
  "license": "NONE",
  "engines": {
    "node": ">=10.0.0",
    "yarn": ">= 1.0.0"
  },
  "web": "dist/index.html",
  "targets": {
    "web": {
      "context": "browser",
      "includeNodeModules": true,
      "outputFormat": "global",
      "engines": {
        "browsers": [
          "last 2 versions"
        ]
      }
    }
  },
  "scripts": {
    "dev:chrome": "NODE_ENV=development TARGET_BROWSER=chrome yarn webpack --watch",
    "dev:firefox": "NODE_ENV=development TARGET_BROWSER=firefox yarn webpack --watch",
    "dev:opera": "NODE_ENV=development TARGET_BROWSER=opera yarn webpack --watch",
    "build:chrome": "NODE_ENV=production TARGET_BROWSER=chrome yarn webpack",
    "build:firefox": "NODE_ENV=production TARGET_BROWSER=firefox yarn webpack",
    "build:opera": "NODE_ENV=production TARGET_BROWSER=opera yarn webpack",
    "build": "yarn run build:chrome && yarn run build:firefox && yarn run build:opera",
    "types:check": "yarn tsc -w",
    "lint": "yarn eslint . --ext .ts,.tsx",
    "lint:fix": "yarn eslint . --ext .ts,.tsx --fix"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.2",
    "@chakra-ui/core": "^0.8.0",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@reach/dialog": "^0.10.5",
    "@types/styled-components": "^5.1.0",
    "@umijs/hooks": "^1.9.3",
    "advanced-css-reset": "^1.1.1",
    "emotion-theming": "^10.0.27",
    "firebase": "^7.15.5",
    "framer-motion": "^1.11.1",
    "lodash": "^4.17.15",
    "parcel": "^2.0.0-nightly.322",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-firebase-hooks": "^2.2.0",
    "react-firebaseui": "^4.1.0",
    "react-hook-form": "^6.0.1",
    "react-is": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-sticky-mouse-tooltip": "^0.0.1",
    "styled-components": "^5.1.1",
    "webext-base-css": "^1.1.0",
    "webextension-polyfill-ts": "^0.16.0",
    "windups": "^1.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/plugin-transform-destructuring": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@parcel/transformer-svg-react": "^2.0.0-nightly.1739",
    "@parcel/transformer-svgo": "^2.0.0-nightly.1739",
    "@parcel/validator-typescript": "^2.0.0-alpha.3",
    "@svgr/webpack": "^5.4.0",
    "@types/classnames": "^2.2.10",
    "@types/lodash": "^4.14.157",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/webpack": "^4.41.17",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.6.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-emotion": "^10.0.27",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "fork-ts-checker-webpack-plugin": "^4.1.6",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^2.0.5",
    "react-devtools": "^4.7.0",
    "resolve-url-loader": "^3.1.1",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^2.3.7",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-extension-reloader": "^1.1.4",
    "wext-manifest-loader": "^2.2.0",
    "wext-manifest-webpack-plugin": "^1.2.1",
    "zip-webpack-plugin": "^3.0.0"
  }
}
